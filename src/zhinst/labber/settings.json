                    "x_unit": "Sample"
                },
                "extend": {},
                "driver": {}
            },
            "scopes/*/result_0": {
                "add": true,
                "indexes": [
                    "dev"
                ],
                "conf": {
                    "label": "Result_0",
                    "datatype": "VECTOR_COMPLEX",
                    "show_in_measurement_dlg": "True",
                    "x_name": "Length",
                    "x_unit": "Sample"
                },
                "driver": {
                    "function": "scopes[*].read",
                    "args": "args[0][0]"
                }
            },
            "scopes/*/result_1": {
                "add": true,
                "indexes": [
                    "dev"
                ],
                "conf": {
                    "label": "Result_1",
